{
  "schemaVersion": 1,
  "id": "reroute",
  "version": "${version}",

  "name": "ReRoute-Fabric",
  "description": "Allows a modded client to be sent directly to the correct server on a BungeeCord network.",
  "authors": [
    {
	  "name": "CM360",
      "contact": {
	    "homepage": "https://github.com/cm-360"
      }
    }
  ],
  "contact": {
    "homepage": "https://github.com/cm-360/ReRoute-Fabric",
    "issues": "https://github.com/cm-360/ReRoute-Fabric/issues",
    "sources": "https://github.com/cm-360/ReRoute-Fabric"
  },

  "license": "CC0-1.0",
  "icon": "assets/reroute/icon.png",

  "environment": "*",
  "entrypoints": {
    "client": [
      "com.github.cm360.reroute.fabric.ReRouteClientMod"
    ],
    "modmenu": [
      "com.github.cm360.reroute.fabric.config.ReRouteConfigScreen"
    ]
  },
  "mixins": [
    "reroute.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  }
}
